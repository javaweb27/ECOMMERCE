{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "padded-blocks": "off",
    "block-spacing": "error",
    "quotes": [
      "warn",
      "double"
    ],
    "semi": [
      "warn",
      "never"
    ],
    "no-extra-semi": "warn",
    "eol-last": "warn",
    "no-undef": "error",
    "eqeqeq": "error",
    "max-len": [
      "warn",
      {
        "code": 127,
        "tabWidth": 2
      }
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "comma-spacing": "warn",
    "array-bracket-newline": [
      "error",
      {
        "multiline": true,
        "minItems": 3
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "indent": [
      "error",
      2
    ],
    "no-trailing-spaces": "error",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ]
  }
}